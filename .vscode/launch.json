  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Chat Server",
      "program": "${workspaceFolder}/src/index.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "envFile": "${workspaceFolder}/.env",
      "outFiles": ["${workspaceFolder}/src/**/*.js"],
      "sourceMaps": true,
      // "console": "integratedTerminal",
      "smartStep": true,
      "skipFiles": [
        // "async_hooks.js",
        // "inspector_async_hook.js",
        "<node_internals>/**/*.js",
        "node_modules/**/*.js"
      ]
    }
  ]
}
